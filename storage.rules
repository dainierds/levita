rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /events/images/{allPaths=**} {
      allow read: if true;
      allow write: if request.auth != null;
    }
    
    // Default rule: block everything else
    match /{allPaths=**} {
      allow read, write: if false;
    }
  }
}
